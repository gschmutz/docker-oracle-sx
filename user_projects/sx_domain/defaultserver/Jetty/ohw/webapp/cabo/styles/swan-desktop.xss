  </style>

  <style selector=".OraSideBar .OraHeaderContent">
     <property name="background-color">#ccd7e0</property>
  </style>

  <style selector=".OraSideBar .OraHeaderContent .p_OraSpacingHeader">
     <property name="background-color">#ccd7e0</property>
  </style>

  <style selector=".OraMessageBox .OraHeaderContent .p_OraSpacingHeader">
    <includeStyle name="LightAccentBackground"/>  
  </style>

  <style selector=".OraSubTabLayoutBody .p_OraSpacingHeader">
     <property name="margin-top">0px</property>
  </style>
  
  <!-- SWAN end header related rules -->

   <style selector=".OraHeaderSub">
    <includeStyle name="HeaderText"/>
    <includeStyle name="HeaderPadding"/>
    <property name="margin-top">1px</property>
    <property name="border-bottom">0px</property>
  </style>
  
  <style selector=".OraHeaderBar">
    <property name="background-repeat">repeat-x</property>
    <property name="background-color">#84b2de</property>
    <property name="border-style">solid</property>
    <property name="border-width">1px 1px 1px 1px</property>
    <property name="border-color">#a3bed8</property>
  </style>

  <style selector=".OraHeaderBarText">
    <property name="color">#343434</property> 
    <includeStyle name="HeaderText" /> 
    <includeStyle name="HeaderPadding" />
    <property name="margin-top">1px</property>
  </style>

   <style selector=".OraHeaderSubSub">
    <includeStyle name="HeaderText"/>
    <property name="padding">0px</property>
    <property name="border-bottom">1px solid #99bedc</property>    
   </style>

  <style selector=".OraErrorText">
    <includeStyle name="DefaultFont"/>
    <includeStyle name="ErrorTextForeground"/>
  </style>

  <!-- mbuk end swan rules -->

  <style selector=".OraErrorHeader">
    <includeStyle selector=".OraHeader"/>
    <property name="border-bottom">0px</property>
    <includeStyle name="ErrorTextForeground"/>
  </style>

  <style selector=".OraInfoHeader">
    <includeStyle selector=".OraHeader"/>
    <property name="border-bottom">0px</property>
    <includeStyle name="InfoTextForeground"/>
  </style>

  <style selector=".OraLightHeaderSub">
    <includeStyle selector=".OraHeaderSub"/>
    <includeStyle name="MediumFont"/>
  </style>

  <style selector=".OraLightHeaderSubSub">
    <includeStyle selector=".OraHeaderSubSub"/>
    <includeStyle name="SmallFont"/>
